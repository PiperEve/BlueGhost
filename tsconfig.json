{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@screens/*": ["src/screens/*"],
			"@api/*": ["src/api/*"],
			"@navigation/*": ["src/navigation/*"],
			"@state/*": ["src/state/*"],
			"@types/*": ["src/types/*"],
			"@utils/*": ["src/utils/*"],
		}
	}
}